{
  "name": "node-benchmark-cli",
  "version": "0.1.1",
  "description": "Simple cross-platform NodeJS CLI tool to benchmark different programs",
  "main": "dist/app.mjs",
  "type": "module",
  "bin": "dist/app.mjs",
  "scripts": {
    "lint": "npx eslint src test",
    "dev": "npx rollup -c --environment NODE_ENV:development -w",
    "prebuild": "rm -rf dist",
    "build": "npx rollup -c --environment NODE_ENV:production",
    "pretest": "npm run lint",
    "test": "node --no-warnings --experimental-vm-modules node_modules/jest/bin/jest.js",
    "prepack": "npm run build",
    "doc": "npx typedoc"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/matteosacchetto/node-benchmark-cli.git"
  },
  "homepage": "https://github.com/matteosacchetto/node-benchmark-cli#README.md",
  "bugs": {
    "url": "https://github.com/matteosacchetto/node-benchmark-cli/issues"
  },
  "author": "Matteo Sacchetto",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-eslint": "^8.0.2",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-run": "^2.1.0",
    "@rollup/plugin-typescript": "^8.3.2",
    "@types/jest": "^28.1.0",
    "@types/node": "^17.0.39",
    "@types/parse-gitignore": "^1.0.0",
    "@types/pidusage": "^2.0.2",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "jest": "^28.1.0",
    "prettier": "^2.6.2",
    "rollup": "^2.75.5",
    "ts-jest": "^28.0.4",
    "typedoc": "^0.22.17",
    "typescript": "^4.7.2"
  },
  "engines": {
    "node": ">=16.15",
    "npm": ">=8.5.5"
  },
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "chalk": "^5.0.1",
    "commander": "^9.3.0",
    "console-table-printer": "^2.11.0",
    "figures": "^4.0.1",
    "ignore": "^5.2.0",
    "ora": "^6.1.2",
    "parse-gitignore": "^2.0.0",
    "pidtree": "^0.6.0",
    "pidusage": "^3.0.0",
    "string-argv": "^0.3.1",
    "strip-ansi": "^7.0.1"
  }
}
